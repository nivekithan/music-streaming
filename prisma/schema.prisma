generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id           BigInt   @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  name         String   @unique
  passwordHash String
  userId       String   @unique
}


model Playlists {
  id         BigInt @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  name String
  music Music[]

}

model Music {
  id BigInt @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  name String
  url String
  playlist Playlists[]
}